{
  "name": "@interval/sdk",
  "version": "0.12.0-dev",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "tar": "yarn pack",
    "build:envoy": "yarn envoy $INIT_CWD/envoy.config.ts $INIT_CWD/.env $INIT_CWD/src/env.ts",
    "build": "yarn build:envoy && tsc",
    "demo:basic": "node ./dist/demos/basic.js",
    "demo:prod": "node ./dist/demos/prodDemo.js",
    "dev": "yarn build:envoy && nodemon --watch src -e ts src/demos/basic.ts"
  },
  "dependencies": {
    "evt": "^1.10.2",
    "uuid": "^8.3.2",
    "ws": "^8.4.1",
    "zod": "^3.13.3"
  },
  "devDependencies": {
    "@interval/envoy": "^1.0.1",
    "@types/dedent": "^0.7.0",
    "@types/node": "^17.0.8",
    "@types/uuid": "^8.3.4",
    "@types/ws": "^8.2.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
